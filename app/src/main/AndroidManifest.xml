<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ss.UserListApplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_user_list"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_user_list_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Week1_0706012010002">
        <activity android:name="com.ss.UserListApplication.UserDetailActivity"></activity>
        <activity android:name="com.ss.UserListApplication.UserEntryActivity" />
        <activity android:name="com.ss.UserListApplication.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>